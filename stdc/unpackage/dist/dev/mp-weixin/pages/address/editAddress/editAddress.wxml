<view class="content"><view class="inputWrapper"><input class="input" placeholder="请填写收货地址" type="text" data-event-opts="{{[['input',[['__set_model',['$0','message','$event',[]],['address']]]]]}}" value="{{address.message}}" bindinput="__e"/></view><view class="inputWrapper"><input class="input" placeholder="请填写收货人" type="text" data-event-opts="{{[['input',[['__set_model',['$0','consignee','$event',[]],['address']]]]]}}" value="{{address.consignee}}" bindinput="__e"/></view><view class="inputWrapper"><input class="input" placeholder="请填写收货人号码" type="text" data-event-opts="{{[['input',[['__set_model',['$0','tel','$event',[]],['address']]]]]}}" value="{{address.tel}}" bindinput="__e"/></view><view class="load-button"><button class="load-button-style" style="float:left;" type="primary" data-event-opts="{{[['tap',[['editButton',['$event']]]]]}}" bindtap="__e">确定</button><block wx:if="{{!address.addressId==''}}"><button class="load-button-style" style="float:right;" type="warn" data-event-opts="{{[['tap',[['delButton',['$event']]]]]}}" bindtap="__e">删除</button></block></view></view>